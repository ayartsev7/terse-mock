    "email": "anton.yartsev@gmail.com",
    "url": "https://github.com/ayartsev7"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.8.0",

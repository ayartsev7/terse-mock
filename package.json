{
  "name": "terse-mock",
  "version": "1.0.11",
  "description": "Easy mock creation and automocking",
  "main": "build/terse-mock.js",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "test",
    "testing",
    "mocking",
    "stub",
    "unittesting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ayartsev7/terse-mock"
  },
  "bugs": {
    "url": "https://github.com/ayartsev7/terse-mock/issues"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc"
  },
  "author": {
    "name": "Anton Yartsev",
    "email": "anton.yartsev@gmail.com",
    "url": "https://github.com/ayartsev7"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.39.0",
    "jest": "^27.3.1",
    "json-summary": "^1.3.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "json-summary"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    }
  }
}
